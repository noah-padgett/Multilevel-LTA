TITLE:  Obligatory BS title;

MONTECARLO:
      NAMES = u1-u4 u21-u24;
      GENERATE = u1-u4 u21-u24 (1);
      CATEGORICAL = u1-u4 u21-u24;
    	GENCLASSES = c1(2) c2(2);
    	CLASSES = c1(2) c2(2);
    	NOBSERVATION = 2000;
    	ncsizes = 1;
    	csizes = 100 (20);
    	NREPS = 2;
      seed=709924;
      save = condition1_rep*.dat;
      repsave = ALL;
ANALYSIS:
    	TYPE=TWOLEVEL MIXTURE;
        STARTS = 20 2;
  MODEL POPULATION:
        %WITHIN%
    	%OVERALL%
         ! regress time 2 on time 1 -> level-1 multinomial regression
         c2#1 ON c1#1*4;
         ! time 1 intercept
         [ c1#1*-.405 ]; !time 1 logit
         ! time 2 intercept (mu)
         [ c2#1*-2.5];

  %BETWEEN%
    	%OVERALL%
    	c2#1 on c1#1*0.3;

       c1#1*0.8;
       c2#1*0.1;

    ! next define the measurement model of the within class response
    !   patterns - wil depend on model structure
    MODEL POPULATION-c1:
    %BETWEEN%
     
        %c1#1%
    	 [u1$1*-.847] (1);
    	 [u2$1*-.847] (2);
    	 [u3$1*-.847] (3);
    	 [u4$1*-.847] (4);

        %c1#2%
    	 [u1$1*0.847] (11);
    	 [u2$1*.847] (12);
    	 [u3$1*.847] (13);
    	 [u4$1*.847] (14);

    MODEL POPULATION-c2:
    %BETWEEN%
        %c2#1%
    	 [u21$1*-.847] (1);
    	 [u22$1*-.847] (2);
    	 [u23$1*-.847] (3);
    	 [u24$1*-.847] (4);

        %c2#2%
    	 [u21$1*.847] (11);
    	 [u22$1*.847] (12);
    	 [u23$1*.847] (13);
    	 [u24$1*.847] (14);
         